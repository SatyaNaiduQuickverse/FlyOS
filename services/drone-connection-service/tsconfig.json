{
    "compilerOptions": {
      "target": "es2019",
      "module": "commonjs",
      "esModuleInterop": true,
      "outDir": "./dist",
      "strict": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "resolveJsonModule": true
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules", "dist"]
  }